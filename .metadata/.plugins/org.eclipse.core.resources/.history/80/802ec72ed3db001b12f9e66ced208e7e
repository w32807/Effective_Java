package enumWithAnnotation.item34;

public class OperationTest {
	public static void main(String[] args) {
		double x = Double.parseDouble("2");
		double y = Double.parseDouble("4");
		
		for(Operation op : Operation.values()) {
			System.out.printf("%f %s %f = %f%n", x, op, y, op.apply(x, y));
		}
		
		
		Operation.fromString("+").isPresent() ? System.out.println(Operation.fromString("+").get()) ? System.out.println("값 없음");
		if(Operation.fromString("++").isPresent()) {System.out.println(Operation.fromString("+").get());}
		
		
		System.out.println(Operation.fromString("+"));
		System.out.println(Operation.fromString("++").get());
	}
	
}
